<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape>
        <gradient
            android:endColor="#42c2af"
            android:startColor="#3083eb" />
        <corners android:radius="10dp" />
        <padding
            android:bottom="2dp"
            android:left="2dp"
            android:right="2dp"
            android:top="2dp" />
    </shape>
</item>
<item>
    <shape>
        <solid android:color="#ffffff" />
        <corners android:radius="10dp" />
    </shape>
</item>
</layer-list>
